{
  "name": "inspirational-goals-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b ./config/tsconfig.json && vite build",
    "preview": "vite preview",
    "lint:js": "eslint . --ext .js,.jsx,.ts,.tsx --config ./config/eslint.config.js",
    "lint:css": "stylelint \"**/*.{css,scss}\" --config ./config/stylelint.config.mjs",
    "lint:css:fix": "stylelint \"**/*.{css,scss}\" --config ./config/stylelint.config.mjs --fix",
    "lint": "pnpm run lint:js && pnpm run lint:css",
    "format": "prettier --write . --config ./config/prettier.config.js",
    "format-check": "prettier --check . --config ./config/prettier.config.js"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.9.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "prettier": "3.6.2",
    "stylelint": "^16.25.0",
    "stylelint-config-recess-order": "^7.4.0",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-order": "^7.0.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7"
  }
}
